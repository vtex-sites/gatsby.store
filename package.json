{
  "name": "gatsby.store",
  "private": true,
  "version": "22.43.11",
  "license": "MIT",
  "browserslist": "supports es6-module and not dead",
  "scripts": {
    "generate": "graphql-codegen",
    "build": "yarn partytown && gatsby build",
    "develop": "yarn partytown && gatsby develop",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "test": "cypress open",
    "lhci": "lhci autorun",
    "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json}\"",
    "lint": "eslint --ext js,ts,jsx,tsx .",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix",
    "postinstall": "is-ci || husky install",
    "partytown": "partytown copylib ./public/~partytown",
    "storybook": "start-storybook --docs -p 6006",
    "build-storybook": "build-storybook",
    "release": "release-it --ci"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.6.1",
    "@envelop/core": "^1.2.0",
    "@envelop/parser-cache": "^2.2.0",
    "@envelop/validation-cache": "^2.2.0",
    "@faststore/api": "^1.12.17",
    "@faststore/graphql-utils": "^1.10.34",
    "@faststore/sdk": "^1.10.34",
    "@faststore/ui": "^1.12.17",
    "@vtex/client-cms": "^0.2.10",
    "gatsby": "^4.16.0",
    "gatsby-plugin-gatsby-cloud": "^4.16.0",
    "gatsby-plugin-manifest": "^4.16.0",
    "gatsby-plugin-netlify": "^5.0.0",
    "gatsby-plugin-next-seo": "^1.10.0",
    "gatsby-plugin-nprogress": "^4.16.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-root-import": "^2.0.8",
    "gatsby-plugin-sass": "^5.16.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "include-media": "^1.4.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intersection-observer": "^9.3.3",
    "sass": "^1.44.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@csmith/release-it-calver-plugin": "^2022.5.13",
    "@cypress/code-coverage": "^3.9.10",
    "@faststore/lighthouse": "^1.11.8",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@lhci/cli": "^0.9.0",
    "@netlify/plugin-gatsby": "^2.0.0-beta",
    "@release-it/conventional-changelog": "^5.0.0",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@testing-library/cypress": "^8.0.0",
    "@types/cypress": "^1.1.3",
    "@vtex/prettier-config": "1.0.0",
    "@vtex/tsconfig": "0.6.0",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.3.3",
    "babel-loader": "^8.2.4",
    "babel-plugin-remove-graphql-queries": "^4.11.1",
    "cypress": "9.6.0",
    "cypress-axe": "^0.13.0",
    "cypress-wait-until": "^1.7.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-vtex-react": "^7.0.0",
    "gatsby-plugin-bundle-stats": "^3.1.3",
    "gatsby-plugin-postcss": "^5.3.0",
    "husky": "^5.2.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^10.5.4",
    "msw": "^0.43.1",
    "msw-storybook-addon": "^1.6.3",
    "postcss": "^8.4.4",
    "prettier": "^2.2.0",
    "release-it": "^15.1.3",
    "storybook-addon-gatsby": "^0.0.5",
    "stylelint": "^14.6.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.3"
  },
  "resolutions": {
    "@typescript-eslint/parser": "^4",
    "@typescript-eslint/eslint-plugin": "^4"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "msw": {
    "workerDirectory": "static"
  }
}
